    "@types/react-router-dom": "^5.1.7",
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "vite": "^2.0.0-beta.5",
    "vite-plugin-mdx": "^0.4.0",
